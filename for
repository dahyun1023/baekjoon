# 1. googoodan

N = int(input())

for i in range(0,9):
  n = N*(i+1)
  print('n')

# 2. A + B - 3

T = int(input())

for i in range(0,T):
  A,B = input().split()

  A = int(A)
  B = int(B)
  print(A+B)
  
# 3. sum

n = int(input())
sum = 0

for i in range(0,n):
  sum += i
print(sum)

